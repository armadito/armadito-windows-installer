<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <A6oRoot>$(SolutionDir)..\..\..\..\</A6oRoot>
    <A6oDeps>$(SolutionDir)..\..\deps\</A6oDeps>
    <A6oInclude>$(A6oRoot)core\libarmadito\include</A6oInclude>
    <GlibBin>$(A6oDeps)glib\Win32\$(Configuration)\bin\</GlibBin>
    <GlibInclude>$(A6oDeps)glib\Win32\$(Configuration)\include;$(A6oDeps)glib\Win32\$(Configuration)\include\glib-2.0;$(A6oDeps)glib\Win32\$(Configuration)\lib\glib-2.0\include</GlibInclude>
    <GlibLib>$(A6oDeps)glib\Win32\$(Configuration)\bin;$(A6oDeps)glib\Win32\$(Configuration)\lib</GlibLib>
    <JsoncInclude>$(A6oDeps)json-c\$(Platform)\$(Configuration)\include</JsoncInclude>
    <JsoncLib>$(A6oDeps)json-c\$(Platform)\$(Configuration)\lib</JsoncLib>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="A6oRoot">
      <Value>$(A6oRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oDeps">
      <Value>$(A6oDeps)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oInclude">
      <Value>$(A6oInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="GlibBin">
      <Value>$(GlibBin)</Value>
    </BuildMacro>
    <BuildMacro Include="GlibInclude">
      <Value>$(GlibInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="GlibLib">
      <Value>$(GlibLib)</Value>
    </BuildMacro>
    <BuildMacro Include="JsoncInclude">
      <Value>$(JsoncInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="JsoncLib">
      <Value>$(JsoncLib)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>